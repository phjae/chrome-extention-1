{
  "manifest_version": 3,
  "name": "Quick Phrases",
  "version": "1.0",
  "description": "Store and quickly retrieve frequently used phrases.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}